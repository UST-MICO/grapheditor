    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "html-webpack-plugin": "^5.3.2",
    "source-map-loader": "^3.0.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.20.37",
    "typescript": "^3.9.9",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0",
    "webpack-merge": "^5.8.0"
  }
